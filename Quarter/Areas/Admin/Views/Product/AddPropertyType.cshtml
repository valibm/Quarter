@model ProductSubCatagory

@{
    List<SubCatagory> propertyTypes = (List<SubCatagory>)ViewData["propertyTypes"];
}

<div class="container">
    <div class="row">
        <div class="col-md-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title mb-2">Add property type to the product</h4>
                    <form enctype="multipart/form-data" method="post" class="forms-sample">
                        <div class="form-group">
                            <label asp-for="SubCatagoryId">Property Types</label>
                            <select asp-for="SubCatagoryId" class="form-control form-control-lg">
                                @foreach (var propertyType in propertyTypes)
                                {
                                    <option value="@propertyType.Id">@propertyType.Name</option>
                                }
                            </select>
                        </div>
                        <button type="submit" class="btn btn-outline-success mr-2">Done</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>