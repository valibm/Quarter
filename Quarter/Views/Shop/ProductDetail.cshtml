@model ShopVM

<!-- BREADCRUMB AREA START -->
<div class="ltn__breadcrumb-area text-left bg-overlay-white-30 bg-image mb-0" data-bs-bg="./assets/img/bg/14.jpg">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="ltn__breadcrumb-inner">
                    <h1 class="page-title">Product Details</h1>
                    <div class="ltn__breadcrumb-list">
                        <ul>
                            <li><a asp-controller="home" asp-action="index"><span class="ltn__secondary-color"><i class="fas fa-home"></i></span> Home</a></li>
                            <li>Product Details</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- BREADCRUMB AREA END -->
<!-- IMAGE SLIDER AREA START (img-slider-3) -->
<div class="ltn__img-slider-area mb-90">
    <div class="container-fluid">
        <div class="row ltn__image-slider-5-active slick-arrow-1 slick-arrow-1-inner ltn__no-gutter-all">
            @foreach (var productImage in Model.Product.ProductImages)
            {
                if (productImage.Image.ForHeader == true)
                {
                    <div class="col-lg-12">
                        <div class="ltn__img-slide-item-4">
                            <a href="~/Assets/Uploads/Images/@productImage.Image.Name" data-rel="lightcase:myCollection">
                                <img src="~/Assets/Uploads/Images/@productImage.Image.Name" alt="Image">
                            </a>
                        </div>
                    </div>
                }
            }
        </div>
    </div>
</div>
<!-- IMAGE SLIDER AREA END -->
<!-- SHOP DETAILS AREA START -->
<div class="ltn__shop-details-area pb-10">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-12">
                <div class="ltn__shop-details-inner ltn__page-details-inner mb-60">
                    <div class="ltn__blog-meta">
                        <ul>
                            <li class="ltn__blog-category">
                                <a href="#">Featured</a>
                            </li>
                            <li class="ltn__blog-category">
                                <a class="bg-orange" href="#">@Model.Product.ProductStatus.Name</a>
                            </li>
                            <li class="ltn__blog-date">
                                <i class="far fa-calendar-alt"></i>@Model.Product.CreatedDate
                            </li>
                            <li>
                                <a href="#"><i class="far fa-comments"></i>@Model.Comments.Count Comments</a>
                            </li>
                        </ul>
                    </div>
                    <h1>@Model.Product.Title</h1>
                    <label><span class="ltn__secondary-color"><i class="flaticon-pin"></i></span>@Model.Product.Area.Name, @Model.Product.Area.Location.City</label>
                    <h4 class="title-2">Description</h4>
                    <p>
                        @Model.Product.Description
                    </p>
                    <p>To the left is the modern kitchen with central island, leading through to the unique breakfast family room which feature glass walls and doors out onto the garden and access to the separate utility room.</p>

                    <h4 class="title-2">Property Detail</h4>
                    <div class="property-detail-info-list section-bg-1 clearfix mb-60">
                        <ul>
                            <li><label>Property ID:</label> <span>@Model.Product.Id</span></li>
                            <li><label>Home Area: </label> <span>@Model.Product.ProductDetails.HomeArea sqft</span></li>
                            <li><label>Rooms:</label> <span>@Model.Product.ProductDetails.Rooms</span></li>
                            <li><label>Baths:</label> <span>@Model.Product.ProductDetails.Baths</span></li>
                            <li><label>Year built:</label> <span>@Model.Product.ProductDetails.YearBuilt</span></li>
                        </ul>
                        <ul>
                            <li><label>Lot Area:</label> <span>@Model.Product.Id </span></li>
                            <li><label>Lot dimensions:</label> <span>@Model.Product.ProductDetails.LotDimensions sqft</span></li>
                            <li><label>Beds:</label> <span>@Model.Product.ProductDetails.Beds</span></li>
                            <li><label>Price:</label> <span>@Model.Product.ProductDetails.Price</span></li>
                            <li><label>Property Status:</label> <span>@Model.Product.ProductStatus.Name</span></li>
                        </ul>
                    </div>

