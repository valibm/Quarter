@model List<AppUser>

<!-- BREADCRUMB AREA START -->
<div class="ltn__breadcrumb-area text-left bg-overlay-white-30 bg-image " data-bs-bg="img/bg/14.jpg">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="ltn__breadcrumb-inner">
                    <h1 class="page-title">Our Agent</h1>
                    <div class="ltn__breadcrumb-list">
                        <ul>
                            <li><a asp-controller="Home" asp-action="Index"><span class="ltn__secondary-color"><i class="fas fa-home"></i></span> Home</a></li>
                            <li>Agent</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- BREADCRUMB AREA END -->
<!-- TEAM AREA START (Team - 3) -->
<div class="ltn__team-area pt-110--- pb-90">
    <div class="container">
        <div class="row justify-content-center">
            @foreach (var appUser in Model)
            {
                <div class="col-lg-4 col-sm-6">
                    <div class="ltn__team-item ltn__team-item-3---">
                        <div class="team-img">
                            <img src="~/Assets/Uploads/Images/@appUser.Image.Name" alt="Image">
                        </div>
                        <div class="team-info">
                            <h4><a asp-controller="Team" asp-action="teamdetail" asp-route-id="@appUser.Id">@appUser.FirstName @appUser.LastName</a></h4>
                            <h6 class="ltn__secondary-color">@appUser.Position</h6>
                            <div class="ltn__social-media">
                                <ul>
                                    <li><a href="@appUser.FacebookLink"><i class="fab fa-facebook-f"></i></a></li>
                                    <li><a href="@appUser.TwitterLink"><i class="fab fa-twitter"></i></a></li>
                                    <li><a href="@appUser.LinkedInLink"><i class="fab fa-linkedin"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<!-- TEAM AREA END -->